{
  "log": {
    "levels": {
      "cli": "warn"
    }
  },
  "features": {
    "odata_new_adapter": false,
    "cds_validate": false
  },
  "fiori": { "draft_compat": true },
  "requires": {
    "[hybrid]": {
      "db": {
        "binding": {
          "type": "cf",
          "apiEndpoint": "https://api.cf.ap10.hana.ondemand.com",
          "org": "sap-btp-org",
          "space": "dev",
          "instance": "my-sap-cap-app-db",
          "key": "my-sap-cap-app-db-key",
          "resolved": false
        },
        "kind": "hana-cloud",
        "vcap": {
          "name": "db"
        }
      }
    }
  }
}
